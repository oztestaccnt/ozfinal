@using System.Web.Razor.Parser

@{
    ViewBag.Title = "Lesson9";
}

<h3>A: @ViewBag.a</h3>
<h3>B: @ViewBag.b</h3>
<h3>TOTAL: @ViewBag.Total</h3>

<h2>@ViewBag.Total</h2>

<form role="form" method="post" action="@Url.Action("Lesson9", "Lessons")">
    <div class="form-group">
        @Html.Label("num1", "First number")
        @Html.TextBox("num1", null, new { @class = "form-control" })
        @*<input type="text" class="form-control" id="num1" name="num1">*@
    </div>
    <div class="form-group">
        @Html.Label("num2", "Second number")
        @Html.TextBox("num2", null, new { @class = "form-control" })
        @*<input type="text" class="form-control" id="num2" name="num2">*@
    </div>
    <button type="submit" class="btn btn-default">Submit</button>

</form>

<ui>
    <h3>List of Names</h3>

    @{
        foreach (var names in ViewBag.Names)
        {
            <li>@names</li>
        }
    }
</ui>



